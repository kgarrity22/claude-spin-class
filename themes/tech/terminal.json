{
  "navigation": [
    "Ctrl+A → Jump to line start",
    "Ctrl+E → Jump to line end",
    "Ctrl+W → Delete word back",
    "Ctrl+U → Clear to line start",
    "Ctrl+K → Clear to line end",
    "Ctrl+L → Clear screen",
    "Ctrl+C → Cancel current command",
    "Ctrl+Z → Suspend process",
    "Ctrl+D → Exit / EOF",
    "Alt+F → Move forward one word",
    "Alt+B → Move back one word",
    "Alt+D → Delete word forward",
    "Ctrl+T → Swap last two chars",
    "Alt+T → Swap last two words",
    "Ctrl+XX → Toggle line start/pos",
    "Ctrl+Y → Paste from kill ring",
    "Tab → Autocomplete",
    "Tab Tab → Show completions",
    "Ctrl+_ → Undo last edit"
  ],
  "history": [
    "Ctrl+R → Reverse history search",
    "Ctrl+G → Exit history search",
    "!! → Repeat last command",
    "!$ → Last arg of prev command",
    "!* → All args of prev command",
    "!^ → First arg of prev command",
    "!foo → Last cmd starting w/ foo",
    "^old^new → Replace in last cmd",
    "Alt+. → Insert last argument",
    "history | grep X → Find command",
    "fc → Edit last cmd in editor",
    "Ctrl+P / ↑ → Previous command",
    "Ctrl+N / ↓ → Next command",
    "cd - → Go to previous directory",
    "dirs -v → Show dir stack"
  ],
  "power": [
    "cmd1 && cmd2 → Run if first OK",
    "cmd1 || cmd2 → Run if first fails",
    "cmd1 ; cmd2 → Run sequentially",
    "cmd1 | cmd2 → Pipe output",
    "cmd > file → Redirect to file",
    "cmd >> file → Append to file",
    "cmd 2>&1 → Stderr to stdout",
    "cmd &> file → All output to file",
    "cmd & → Run in background",
    "jobs → List background jobs",
    "fg %1 → Bring job to foreground",
    "kill %1 → Kill background job",
    "$(cmd) → Command substitution",
    "${var:-default} → Default value",
    "set -e → Exit on error",
    "trap 'cmd' EXIT → Cleanup on exit",
    "time cmd → Time a command",
    "watch -n2 cmd → Repeat every 2s",
    "tee file → Pipe AND save output",
    "xargs → Build args from stdin"
  ]
}
